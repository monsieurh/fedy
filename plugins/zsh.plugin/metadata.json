{
	"icon": "zsh",
	"label": "Zsh+OhMyZsh",
	"description": "Zsh shell + OhMyZsh",
	"license": "Free",
	"category": "Programming",
	"scripts": {
		"exec": {
			"label": "Install",
			"command": "run-as-root -s zsh_ohmyzsh.sh"
